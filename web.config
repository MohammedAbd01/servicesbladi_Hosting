<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <system.webServer>
    <handlers>
      <add name="PythonHandler" path="*" verb="*" modules="FastCgiModule" scriptProcessor="C:\Program Files\Python310\python.exe|D:\home\site\wwwroot\backend\servicesbladi\wsgi.py" resourceType="Unspecified" />
    </handlers>
    <rewrite>
      <rules>
        <rule name="Static Files" stopProcessing="true">
          <match url="^static/(.*)" />
          <action type="Rewrite" url="backend/staticfiles/{R:1}" />
        </rule>
      </rules>
    </rewrite>
  </system.webServer>
</configuration>
